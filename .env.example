# Database Configuration
POSTGRES_DB=n8n
POSTGRES_USER=n8n
POSTGRES_PASSWORD=CHANGE_ME_secure_password
POSTGRES_NON_ROOT_USER=n8n
POSTGRES_NON_ROOT_PASSWORD=CHANGE_ME_secure_password
POSTGRES_PORT=5432

# Redis Configuration
REDIS_DB=0
REDIS_PORT=6379

# n8n Configuration
# IMPORTANT: Generate secure keys with: openssl rand -hex 32
N8N_ENCRYPTION_KEY=CHANGE_ME_32_char_hex_string
N8N_JWT_SECRET=CHANGE_ME_32_char_hex_string
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
N8N_WEBHOOK_PORT=5679
WEBHOOK_URL=http://localhost:5678/

# General Configuration
TIMEZONE=UTC
N8N_LOG_LEVEL=info
EXECUTIONS_MODE=queue

# Task Runners and Execution Configuration (Recommended)
N8N_RUNNERS_ENABLED=true
OFFLOAD_MANUAL_EXECUTIONS_TO_WORKERS=true
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true

# Optional: n8n User Management (uncomment to enable)
# N8N_USER_MANAGEMENT_DISABLED=false
# N8N_EMAIL_MODE=smtp
# N8N_SMTP_HOST=smtp.gmail.com
# N8N_SMTP_PORT=587
# N8N_SMTP_USER=your-email@gmail.com
# N8N_SMTP_PASS=your-app-password
# N8N_SMTP_SENDER=your-email@gmail.com
# N8N_SMTP_SSL=false

# Optional: External Services
# N8N_EXTERNAL_HOOK_FILES=/home/node/.n8n/external-hooks/external-hooks.js

# Optional: Metrics and Monitoring
# N8N_METRICS=true
# N8N_METRICS_PREFIX=n8n_

# Optional: Security
# N8N_SECURE_COOKIE=false
# N8N_SKIP_WEBHOOK_DEREGISTRATION_SHUTDOWN=false

# AI Services Configuration
# ==========================

# Qdrant Vector Database
QDRANT_HTTP_PORT=6333
QDRANT_GRPC_PORT=6334
QDRANT_LOG_LEVEL=INFO

# Flowise AI Agent Builder
FLOWISE_PORT=3001
# Optional: Set username and password for Flowise UI access
FLOWISE_USERNAME=admin
FLOWISE_PASSWORD=CHANGE_ME_secure_password

# Neo4j Graph Database
NEO4J_AUTH=neo4j/CHANGE_ME_password
NEO4J_HTTP_PORT=7474
NEO4J_HTTPS_PORT=7473
NEO4J_BOLT_PORT=7687
NEO4J_PAGECACHE_SIZE=512M
NEO4J_HEAP_INITIAL=512M
NEO4J_HEAP_MAX=1G

# Langfuse LLM Observability
LANGFUSE_PORT=3002
# IMPORTANT: Generate secure keys with: openssl rand -hex 32
LANGFUSE_SALT=CHANGE_ME_64_char_hex_string
LANGFUSE_NEXTAUTH_SECRET=CHANGE_ME_64_char_hex_string
LANGFUSE_ENCRYPTION_KEY=CHANGE_ME_64_char_hex_string
LANGFUSE_TELEMETRY_ENABLED=false
LANGFUSE_ENABLE_EXPERIMENTAL_FEATURES=true

# Langfuse Initial User Setup
LANGFUSE_INIT_ORG_NAME=Default_Organization
LANGFUSE_INIT_PROJECT_NAME=Default_Project
LANGFUSE_INIT_USER_EMAIL=admin@example.com
LANGFUSE_INIT_USER_NAME=Admin_User
LANGFUSE_INIT_USER_PASSWORD=CHANGE_ME_admin_password

# ClickHouse Analytics Database
CLICKHOUSE_HTTP_PORT=8123
CLICKHOUSE_NATIVE_PORT=9000
CLICKHOUSE_PASSWORD=CHANGE_ME_clickhouse_password

# MinIO S3-Compatible Storage
MINIO_API_PORT=9002
MINIO_CONSOLE_PORT=9001
MINIO_ROOT_PASSWORD=CHANGE_ME_minio_password

# Supabase Database Platform
SUPABASE_STUDIO_PORT=3000
SUPABASE_API_PORT=8000
# IMPORTANT: Generate secure JWT secret with: openssl rand -hex 32
SUPABASE_JWT_SECRET=CHANGE_ME_32_char_hex_string
SUPABASE_ANON_KEY=CHANGE_ME_supabase_anon_jwt_token
SUPABASE_SERVICE_ROLE_KEY=CHANGE_ME_supabase_service_role_jwt_token
SUPABASE_POSTGRES_PASSWORD=CHANGE_ME_supabase_db_password
SUPABASE_DASHBOARD_USERNAME=supabase
SUPABASE_DASHBOARD_PASSWORD=CHANGE_ME_dashboard_password

# Git Configuration
# These will be set automatically when you run: just git-config
GIT_AUTHOR_NAME="Your Name"
GIT_AUTHOR_EMAIL="your.email@example.com"
GIT_COMMITTER_NAME="Your Name"
GIT_COMMITTER_EMAIL="your.email@example.com"