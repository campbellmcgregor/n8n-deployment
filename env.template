LANGFUSE_SALT=your_langfuse_salt_32_chars_minimum
LANGFUSE_NEXTAUTH_SECRET=your_nextauth_secret_32_chars_minimum
LANGFUSE_ENCRYPTION_KEY=your_encryption_key_32_chars_minimum
LANGFUSE_TELEMETRY_ENABLED=false
LANGFUSE_ENABLE_EXPERIMENTAL_FEATURES=true

